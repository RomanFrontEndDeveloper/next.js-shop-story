{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@/*": ["app/*"]
		},
		"jsx": "preserve",
		"allowJs": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"strict": false
	},
	"include": ["next-env.d.ts", "**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
